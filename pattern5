public class pattern5{
	public static void main(String args[]){
		int n=5,i,j,r,m=2,s=4;
		for(i=1;i<=n;i++){
			for(j=n-i;j>0;j--){
				System.out.print(" ");
			}if(i==1)System.out.print("5");
			else{
				for(r=1;r<i+m;r++){
					System.out.print(s);
				}s--;
				m++;
				
			}System.out.println();
		}
	}

}
/*
    5
   444
  33333
 2222222
111111111
*/
